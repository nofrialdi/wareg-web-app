{
  "name": "wareg",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "test": "echo No tests found.",
    "format": "prettier --write --ignore-path=.gitignore .",
    "format:check": "prettier --check --ignore-path=.gitignore .",
    "deploy:vercel": "vercel",
    "precommit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@types/node": "20.4.1",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.9",
    "next": "13.4.9",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.0.0",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "@tanstack/react-query": "^4.0.0",
    "@tanstack/react-query-devtools": "^4.0.0",
    "autoprefixer": "^10.0.0",
    "eslint-config-prettier": "^8.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^13.2.3",
    "postcss": "^8.0.0",
    "prettier": "^2.8.8",
    "react-icons": "^4.8.0",
    "tailwindcss": "^3.0.0",
    "vercel": "^30.2.2"
  }
}
